{
    "version":2,
    "builds":[

        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest":"/"
        }
    ],
    "env": {
        "DATABASE_URL": "${JSON.stringify(require('./config/production.json').db)}"
      }
}
