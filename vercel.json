{
    "version":2,
    "builds":[

        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest":"/"
        }
    ],
    
    "env": {
        "NODE_ENV": "production",
        "DATABASE_URL": "./config"
      } 
      ,
        "rewrites": [
          {
            "source": "/socket.io/(.*)",
            "destination": "/socket.io"
          }
        ]
      
      
      
}
